{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "default": {},
    "title": "Root Schema",
    "required": [
        "username",
        "password",
        "isSeller"
    ],
    "properties": {
        "username": {
            "type": "string",
            "default": "",
            "title": "The username Schema",
            "examples": [
                "bob"
            ]
        },
        "password": {
            "type": "string",
            "default": "",
            "title": "The password Schema",
            "examples": [
                "12ad"
            ]
        },
        "isSeller": {
            "type": "boolean",
            "default": true,
            "title": "The isSeller Schema",
            "examples": [
                true
            ]
        },
        "address": {
            "type": "string",
            "default": "",
            "title": "The address Schema",
            "examples": [
                "158 brickwood ave"
            ]
        }
    },
    "examples": [{
        "username": "bob",
        "password": "12ad",
        "isSeller": true,
        "address": "158 brickwood ave"
    }]
}